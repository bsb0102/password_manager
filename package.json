{
    "name": "pwd-manager",
    "version": "1.0.0",
    "description": "Password Manager",
    "main": "backend/App.js",
    "scripts": {
        "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
        "start:backend": "nodemon server.js",
        "start:frontend": "cd frontend && react-app-rewired start",
        "build:frontend": "cd frontend && react-scripts build",
        "build:backend": "your-build-command-here",
        "build": "npm run build:frontend && npm run build:backend",
        "test:frontend": "cd frontend && npm test",
        "test:backend": "your-test-command-here",
        "test": "npm run test:frontend && npm run test:backend"
    },
    "dependencies": {
        "axios": "^1.6.3",
        "bcrypt": "^5.1.1",
        "bootstrap": "^5.3.2",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "crypto-js": "^4.2.0",
        "csurf": "^1.11.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-validator": "^7.0.1",
        "fs": "^0.0.1-security",
        "https": "^1.0.0",
        "iptable": "^1.0.0",
        "jsonwebtoken": "^9.0.2",
        "kill-port": "^2.0.1",
        "mongoose": "^8.0.3",
        "openssl": "^2.0.0",
        "qrcode": "^1.5.3",
        "react": "^18.2.0",
        "react-router": "^6.21.1",
        "react-router-dom": "^6.0.0",
        "react-scripts": "^5.0.1",
        "speakeasy": "^2.0.0",
        "url-encode-decode": "^1.0.0"
    },
    "devDependencies": {
        "chai": "^5.0.0",
        "chai-http": "^4.4.0",
        "concurrently": "^8.2.2",
        "mocha": "^10.2.0",
        "nodemon": "^3.0.2",
        "react-app-rewired": "^2.2.1"
    }
